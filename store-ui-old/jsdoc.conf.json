{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "src"
    ],
    "includePattern": "\\.(jsx|js|ts|tsx)$"
  },
  "plugins": [
    "node_modules/better-docs/typescript",
    "node_modules/better-docs/component",
    "plugins/markdown"
  ],
  "templates": {
    "better-docs": {
      "name": "My React components"
    }
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  }
}
